{
  "name": "tmagic-playground",
  "version": "1.1.4",
  "private": true,
  "scripts": {
    "clean:top": "rimraf dist",
    "dev": "vite --mode dev",
    "dev:vue2": "vite --mode vue2",
    "dev:react": "vite --mode react",
    "build": "npm run clean:top && node --max_old_space_size=8192 node_modules/vite/bin/vite.js build --mode vue3",
    "build:uat": "npm run clean:top && node --max_old_space_size=8192 node_modules/vite/bin/vite.js build --mode uat",
    "build:dev": "npm run clean:top && node --max_old_space_size=8192 node_modules/vite/bin/vite.js build --mode dev",
    "serve": "vite preview"
  },
  "dependencies": {
    "@bdwjs/localstorage": "^1.1.0",
    "@element-plus/icons-vue": "^2.0.9",
    "@tmagic/editor": "1.1.4",
    "@tmagic/form": "1.1.4",
    "@tmagic/schema": "1.1.4",
    "@tmagic/stage": "1.1.4",
    "@tmagic/utils": "1.1.4",
    "axios": "^0.25.0",
    "element-plus": "^2.2.6",
    "js-md5": "^0.7.3",
    "lodash-es": "^4.17.21",
    "monaco-editor": "^0.34.0",
    "nanoid": "^4.0.0",
    "pinia": "^2.0.22",
    "serialize-javascript": "^6.0.0",
    "terser": "^5.14.2",
    "ua-parser-js": "^1.0.2",
    "vue": "^3.2.37",
    "vue-router": "^4.0.10"
  },
  "devDependencies": {
    "@types/js-md5": "^0.4.3",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^15.12.4",
    "@types/serialize-javascript": "^5.0.1",
    "@types/ua-parser-js": "^0.7.36",
    "@vitejs/plugin-legacy": "^2.2.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/compiler-sfc": "^3.2.37",
    "sass": "^1.35.1",
    "typescript": "^4.7.4",
    "vite": "^3.1.3",
    "vue-tsc": "^0.39.4"
  }
}
